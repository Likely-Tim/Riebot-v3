{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended", "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": [
        "/docs"
    ],
    "rules": {
        "require-jsdoc": "off",
        "max-len": "off",
        "no-throw-literal": "off",
        "no-constant-condition": "off"
    }
}
